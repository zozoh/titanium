{
  "entries" : [{
        "path": "src/com",
        "prefix": "ti/com/",
        "includes": "^(hmaker|ti|net|web|wn)/.+$",
        "target": "all"
      }, {
        "path": "src/mod",
        "prefix": "ti/mod/",
        "includes": "^(hmaker|ti|wn)/.+$",
        "target": "all"
      }, {
        "path": "src/lib/www",
        "prefix": "ti/lib/www/",
        "target": "all"
      }, {
        "path": "src/app",
        "prefix": "/a/load/",
        "ignore": "^.+\/(pc_tmpl.*\\.html|_app.json|init_context)$",
        "target": "all"
      }, {
        "path": "src/i18n",
        "prefix": "ti/i18n/",
        "target": "all"
      }],
  "targets" : {
    "all": "src/dist/ti-more-all.js"
  }
}