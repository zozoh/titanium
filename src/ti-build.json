{
  "entries" : [{
        "path": "core/ti.mjs",
        "target": "core",
        "version": "^( *\"version\" *: *\")([0-9.]+)-dev(\" *, *)$"
      }, {
        "path": "lib/walnut/walnut.mjs",
        "target": "walnut",
        "version": "^(const WALNUT_VERSION = \")([0-9.]+)-dev(\")$"
      }, {
        "path": "com",
        "prefix": "ti/com/",
        "includes": "^(hmaker|ti|net|web|wn)/.+$",
        "target": "all"
      }, {
        "path": "mod",
        "prefix": "ti/mod/",
        "includes": "^(hmaker|ti|wn)/.+$",
        "target": "all"
      }, {
        "path": "lib/www",
        "prefix": "ti/lib/www/",
        "target": "all"
      }, {
        "path": "app",
        "prefix": "/a/load/",
        "ignore": "^.+\/(pc_tmpl.*\\.html|_app.json|init_context)$",
        "target": "all"
      }, {
        "path": "i18n",
        "prefix": "ti/i18n/",
        "target": "all"
      }],
  "targets" : {
    "core": {
      "path" : "dist/es6/ti-core.js",
      "wrap" : false
    },
    "walnut": {
      "path" : "dist/es6/ti-walnut.js",
      "wrap" : false
    },
    "all": {
      "path" : "dist/es6/ti-more-all.js",
      "wrap" : true
    }
  }
}