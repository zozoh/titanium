{
  "comIcon" : "im-archive",
  "comType" : "@com:wn/adaptlist",
  "actions" : [{
      "name" : "reloading",
      "type" : "action",
      "icon" : "zmdi-refresh",
      "text" : "i18n:refresh",
      "altDisplay" : {
        "icon" : "zmdi-refresh zmdi-hc-spin",
        "text" : "i18n:loading"
      },
      "action" : "dispatch:current/reload",
      "shortcut" : "CTRL+SHIFT+R",
      "wait" : 500
    },{
      "type":"line"
    }, {
      "type" : "group",
      "icon" : "im-menu",
      "items" : [{
          "name" : "create",
          "type" : "action",
          "icon" : "zmdi-flare",
          "text" : "i18n:create",
          "action" : "main:doCreate",
          "shortcut" : "ALT+SHIFT+N"
        },{
          "name" : "deleting",
          "type" : "action",
          "icon" : "zmdi-delete",
          "text" : "i18n:del",
          "altDisplay" : {
            "icon" : "zmdi-refresh zmdi-hc-spin",
            "text" : "i18n:del-ing"
          },
          "action" : "main:doDelete",
          "shortcut" : "CTRL+DELETE"
        },{
          "type":"line"
        },{
          "name" : "renaming",
          "type" : "action",
          "icon" : "zmdi-space-bar",
          "text" : "i18n:rename",
          "altDisplay" : {
            "icon" : "zmdi-refresh zmdi-hc-spin"
          },
          "action" : "main:doRename",
          "shortcut" : "F2"
        },{
          "type":"line"
        },{
          "name" : "selectAll",
          "type" : "action",
          "icon" : "zmdi-check-all",
          "text" : "i18n:select-all",
          "action" : "main:invokeList(checkRow)",
          "shortcut" : "CTRL+A"
        },{
          "name" : "cancelAll",
          "type" : "action",
          "__icon" : "zmdi-minus",
          "text" : "i18n:cancel-all",
          "action" : "main:invokeList(cancelRow)"
        },{
          "type" : "line"
        },{
          "name" : "exposeHidden",
          "type" : "action",
          "icon" : "zmdi-eye",
          "text" : "i18n:wn-expose-hidden-on",
          "altDisplay" : {
            "icon" : "zmdi-eye-off",
            "text" : "i18n:wn-expose-hidden-off",
            "capture" : false
          },
          "action" : "main:toggleExposeHidden",
          "shortcut" : "ALT+CTRL+SHIFT+H"
        },{
          "type" : "line"
        },{
          "name" : "upload",
          "type" : "action",
          "icon" : "zmdi-cloud-upload",
          "text" : "i18n:upload-file",
          "__action" : "commit:main/showUploadFilePicker",
          "action" : "main:openLocalFileSelectdDialog",
          "shortcut" : "ALT+CTRL+SHIFT+U"
        },{
          "type" : "line"
        },{
          "type" : "action",
          "icon" : "zmdi-download",
          "text" : "i18n:download-to-local",
          "action" : "main:doDownload"
        },{
          "type" : "line"
        },{
          "type" : "action",
          "icon" : "zmdi-namebard",
          "text" : "i18n:console",
          "action" : "=>Ti.Be.Open('/a/open/wn.console')",
          "shortcut" : "ALT+SHIFT+T"
        },{
          "type" : "action",
          "icon" : "zmdi-info-outline",
          "text" : "i18n:properties",
          "action" : "main:openCurrentMeta",
          "shortcut" : "ALT+SHIFT+P"
        }]
    }]
}