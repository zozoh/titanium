{
  "comType" : "ti-wizard",
  "comConf" : {
    "current" : 1,
    "value" : "=data",
    "steps" : [{
        "title": "基本信息",
        "comType" : "ti-form",
        "comConf" : {
          "fields" : [{
              "title": "姓名",
              "name" : "name",
              "comType" : "ti-input"
            }, {
              "title": "年龄",
              "name" : "age",
              "type" : "Integer",
              "comType" : "ti-input-num"
            }, {
              "title": "伟大",
              "name" : "great",
              "type" : "Boolean",
              "comType" : "ti-toggle"
            }, {
              "title": "居所",
              "name" : "location",
              "display": "location",
              "comType" : "ti-input"
            }],
          "data": ":=.."
        },
        "prev" : false,
        "next" : {
          "enabled" : {
            "age"   : "inRange(40,50)",
            "name"  : "!isBlank",
            "great" : "isTrue",
            "location"  : "notBlank"
          }
        }
      }, {
        "title": "联系方式",
        "comType" : "ti-form",
        "comConf" : {
          "fields" : [{
              "title": "手机",
              "name" : "phone",
              "comType" : "ti-input"
            }, {
              "title": "邮箱",
              "name" : "email",
              "comType" : "ti-input"
            }],
          "data": ":=.."
        },
        "prev" : true,
        "next" : {
          "enabled" : {
            "phone" : "!isBlank",
            "email" : "!isBlank"
          }
        }
      }, {
        "title": "特殊标签",
        "comType" : "ti-form",
        "comConf" : {
          "fields" : [{
              "name" : "lbls",
              "type" : "Array",
              "comType" : "ti-input-tags"
            }],
          "data": ":=.."
        },
        "prev" : true,
        "next" : {
          "icon" : "far-check-circle",
          "text" : "完成",
          "enabled" : {
            "lbls" : "!isEmpty"
          }
        }
      }]
  },
  "dispatchActions" : {
    "change" : "updatePair",
    "merge" : "update",
    "goto-step" : {
      "action" : "updateComConf",
      "payload" : {
        "current" : "=$args.index"
      }
    }
  }
}