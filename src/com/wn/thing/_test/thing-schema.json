{
  "list" : {
    "comType" : "ti-table",
    "comConf" : {
      "list" : "@search.list",
      "border" : true,
      "checkable" : true,
      "multi"  : true,
      "fields" : [{
        "key"   : "th_nm",
        "title" : "名称"
      }, {
        "key"   : "ct",
        "title" : "创建时间"
      }]
    }
  },
  "meta" : {
    "comType" : "wn-obj-form",
    "comConf" : {
      "data"   : "@current.meta",
      "status" : "@current.status",
      "fieldStatus" : "@current.fieldStatus",
      "config" : {
        "fields" : [{
          "name"    : "th_nm",
          "comType" : "ti-input"
        },{
          "name" : "id"

        }]
      },
      "updateBy" : {
        "method" : "dispatch",
        "target" : "main/current/updateMeta"
      },
      "setFieldStatusBy" : {
        "method" : "commit",
        "target" : "main/config/setMetaFieldStatus"
      }
    }
  },
  "content" : {
    "comType" : "wn-obj-puretext",
    "comConf" : {
      "meta"         : "@current.meta",
      "content"      : "@current.content",
      "savedContent" : "@current.__saved_content",
      "contentType"  : "@current.contentType",
      "status"       : "@current.status"
    }
  }
}