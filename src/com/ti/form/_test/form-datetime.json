          "name"  : "date",
          "comType" : "ti-input-date",
          "comConf" : {
            "editable" : false,
            "displayFormat" : "yyyy年M月d日"
          }
        }, {
          "title" : "日期+时间",
          "name"  : "datetime",
          "comType" : "ti-input-datetime",
          "comConf" : {
            "editable" : true,
            "displayFormat" : "yyyy年M月d日 HH:mm:ss"
          }
        }]
      },
    "data" : {
      "time"  : "=time",
      "month" : "=month",
