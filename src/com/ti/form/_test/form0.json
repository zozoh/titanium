{
  "json": true,
  "dspacing" : "tiny",
  "fields" : [{
      "icon"    : "zmdi-flattr",
      "title"   : "ID",
      "name"    : "id",
      "comType" : "ti-label"
    }, {    
      "name" : "name"
    }, {
      "name" : "great",
      "type" : "Boolean",
      "comType" : "ti-toggle"
    }, {
      "name" : "nb_test",
      "type" : "Integer",
      "width" : 120,
      "comType" : "ti-input-num"
    }, {
      "name" : "lbls",
      "type" : "Array",
      "tip" : "xxxxxx"
    }, {
      "icon" : "zmdi-pin",
      "title" : "地理位置",
      "name" : ["lng", "lat"],
      "type" : "Object",
      "comType" : "ti-lbs-map",
      "comConf" : {
        "by" : "baidu",
        "width" : "100%",
        "zoom"  : 16
      }
    }, {
      "name" : "avatar",
      "type" : "Object",
      "serializer" : {
         "name" : "formatStringBy",
         "args" : "id:${id}"
      },
      "comType" : "wn-obj-picker",
      "comConf" : {}
    }, {
      "name" : "status",
      "type" : "String",
      "title" : "状态",
      "comType" : "ti-switcher",
      "comConf" : {
        "multi" : false,
        "options" : [
          {"value":"live", "text":"生"},
          {"value":"quanta", "text":"量子"},
          {"value":"dead", "text":"死"}]
      }
    }, {
      "name" : "profession",
      "type" : "Array",
      "comType" : "ti-droplist",
      "comConf" : {
        "multi" : true,
        "options" : [
          {"value":"A", "text":"程序员"},
          {"value":"B", "text":"架构师"},
          {"value":"C", "text":"设计师"},
          {"value":"D", "text":"企业家"}]
      }
    }, {
      "name" : "home",
      "type" : "String",
      "comType" : "ti-droplist",
      "comConf" : {
        "cached" : true,
        "options" : {
          "method" : "Wn.Sys.exec2",
          "args"   : ["obj ~/* -cqn -e '^(id|nm|title|icon)$' -l -json"]
        },
        "defaultIcon" : "",
        "mapping" : {
          "icon"  : "icon",
          "text"  : ["title", "nm"],
          "value" : "id"
        }
      }
    }, {
      "name" : "birthday",
      "type" : "DateTime",
      "serializer"  : {"name":"formatDate", "args":["yyyyMMdd"]},
      "transformer" : {"name":"formatDate", "args":["yyyy-MM-dd"]}
    }, {
      "type" : "Group",
      "title" : "基本信息",
      "fields" : [{
          "name" : "age",
          "type" : "Number"
        }, {
          "name" : "stature",
          "type" : "Number"
        }, {
          "name" : "weight",
          "type" : "Number"
        }]
    }]
}