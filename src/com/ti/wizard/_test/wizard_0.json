{
  "comType" : "ti-wizard",
  "comConf" : {
    "current" : 0,
    "data"  : "=data",
    "steps" : [{
        "comType" : "ti-form",
        "comConf" : {
          "config" : {
            "fields" : [{
                "name" : "theName",
                "comType" : "ti-input"
              }, {
                "name" : "theValue",
                "comType" : "ti-switcher",
                "comConf" : {
                  "options" : [1,2,3,5,8]
                }
              }]
          },
          "data" : {
            "theName"  : "=data.theName",
            "theValue" : "=data.theValue"
          }
        },
        "comEvents" : {
          "changed" : true
        },
        "prev" : false,
        "next" : {
          "enabled" : {
            "theValue" : "isNumber",
            "theName"  : "NoEmpty"
          }
        }
      }, {
        "dataKey" : "pos",
        "comType" : "ti-form",
        "comConf" : {
          "config" : {
            "fields" : [{
                "name" : "x",
                "type" : "Integer",
                "width" : "1.4rem",
                "comType" : "ti-input-num",
                "comConf" : {
                  "step" : 100
                }
              }, {
                "name" : "y",
                "type" : "Integer",
                "width" : "1.4rem",
                "comType" : "ti-input-num",
                "comConf" : {
                  "step" : 10
                }
              }]
          },
          "data" : {
            "x"  : "=data.pos.x",
            "y" : "=data.pos.y"
          }
        },
        "comEvents" : {
          "changed" : {
            "emitName"    : "merge",
            "transformer" : "toObjByPair"
          }
        },
        "prev" : true,
        "next" : {
          "enabled" : {
            "pos.x" : "isNumber",
            "pos.y" : "isNumber"
          }
        }
      }, {
        "prev" : true,
        "next" : true
      }, {
        "prev" : true,
        "next" : true
      }, {
        "prev" : true,
        "next" : true
      }]
  },
  "dispatchActions" : {
    "changed" : "updatePair",
    "merge" : "update",
    "goto-step" : {
      "action" : "updateComConf",
      "payload" : {
        "current" : "=$args.index"
      }
    }
  }
}