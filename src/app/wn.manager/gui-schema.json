{
  "methods" : "@Home:archives/archives-methods.mjs",
  "pcSkyLogo" : {
    "comType" : "ti-icon",
    "comConf" : {
      "width" : "3em", 
      "fontSize" : "1.5em", 
      "value" : "=theLogo"
    }
  },
  "pcSkyTitle" : {
    "comType" : "ti-crumb",
    "comConf" : "=theCrumb"
  },
  "pcSkyUserInfo" : {
    "comType" : "ti-web-session-badge",
    "comConf" : "=theSessionBadge"
  },
  "pcSkyMenu" : {
    "comType" : "ti-menu",
    "comConf" : "=theMenu"
  },
  "pcMainSideBar" : {
    "comType" : "wn-gui-side-nav",
    "comConf" : {
      "items" : "=sidebar",
      "highlightItemId" : "=theMetaId"
    }
  },
  "pcMainArena" : {
    "comType" : "wn-gui-arena",
    "comConf" : "=theArena"
  },
  "pcFooter" : {
    "comType" : "wn-gui-footer",
    "comConf" : {
      "infoIcon"   : "=mainComIcon",
      "infoText"   : "=mainComType",
      "statusText" : "=mainStatusText"
    }
  }
}