{
  "name" : "wn.manager",
  "i18n" : "@i18n:wn-manager",
  "template" : "./wn-manager.html",
  "computed" : [{
    "...session" : {
      "getters" : {
        "me" : "get"
      }
    },
    "...meta" : {
      "getters" : {
        "obj" : "get"
      }      
    }
  }],
  "methods" : {
    "reloadMain" : "./reload_main.mjs"
  },
  "mixins" : ["./wn-manager.mjs"],
  "store" : {
    "modules" : {
      "session" : "@mod:wn/session", 
      "meta"    : "@mod:wn/obj-meta"
    },
    "plugins" : ["./store_watcher.mjs"]
  },
  "components" : [
    "./com/wnm-sidebar-item",
    "@com:ti/icon",
    "@com:ti/layout",
    "@com:ti/loading",
    "@com:ti/menu",
    "@com:ti/obj/thumb",
    "@com:ti/obj/meta",
    "@com:ti/list/tiles",
    "@com:ti/user/info"],
  "deps" : ["@lib:walnut"]
}