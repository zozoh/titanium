          "className": "page-head",
          "body" : {
            "comType": "WebMediaImage",
            "comConf": {
              "style": {
                "width":"10.55rem", "margin": "0 auto",
                "borderRadius": ".2rem",
                "overflow": "hidden",
                "boxShadow": "5px 5px 20px rgba(0,0,0,0.4)",
                "marginBottom": "6em"
              },
              "src": "->${base}img/home-gvp.png"}
          }
        },
        "@BLOCK(desktop.footer)"],
      "panels" : [
        "@BLOCK(desktop.login)",
        "@BLOCK(desktop.sky)"]
    },
    "tablet" : "desktop",
    "phone"  : "desktop"
