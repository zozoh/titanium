{
  "methods" : null,
  "filter" : {
    "comType" : "wn-thing-filter",
    "comConf" : {
      "placeholder" : "请输入角色名过滤",
      "status" : "=status"
    }
  },
  "list" : {
    "comType" : "wn-table",
    "comConf" : {
      "data" : "=search.list",
      "changedId"  : "=changedRowId",
      "currentId"  : "=search.currentId",
      "checkedIds" : "=search.checkedIds",
      "checkable" : true,
      "multi"  : true,
      "fields" : [{
        "title" : "角色名",
        "width" : -200,
        "display" : ["@<thumb:far-smile>", "th_nm"]
      }, {
        "title" : "角色值",
        "width" : -80,
        "display" : "nm"
      }, {
        "title" : "默认角色",
        "width" : -60,
        "display" : {
          "key" : "isdft",
          "transformer" : {
            "name" : "toStr",
            "args" : [["--", "是"]]
          }
        }
      }, {
        "title" : "着陆页",
        "display" : "mainpage"
      }]
    }
  },
  "pager" : {
    "comType" : "ti-paging-jumper",
    "comConf" : {
      "value" : "=search.pager"
    }
  },
  "creator" : {
    "comType" : "wn-thing-creator",
    "comConf" : {
      "fields" : [{
        "name" : "th_nm",
        "title" : "角色名",
        "comType" : "ti-input",
        "comConf" : {}
      }, {
        "name" : "nm",
        "title" : "角色值",
        "comType" : "ti-input",
        "comConf" : {}
      }],
      "data" : {
        "th_nm" : "普通用户",
        "nm" : "user"
      }
    }
  },
  "meta" : {
    "comType" : "wn-obj-form",
    "comConf" : {
      "data"   : "=current.meta",
      "blankAs" : {
        "icon" : "zmdi-arrow-left",
        "text" : "请选择一个角色查看详情"
      },
      "status" : "=current.status",
      "fieldStatus" : "=current.fieldStatus",
      "fields" : [{
          "title"  : "ID",
          "name"   : "id"
        }, {
          "title"   : "角色名",
          "name"    : "th_nm",
          "comType" : "ti-input"
        },{
          "title"   : "角色值",
          "name"    : "nm",
          "comType" : "ti-input"
        },{
          "title"   : "默认角色",
          "name"    : "isdft",
          "type"    : "Boolean",
          "comType" : "ti-toggle"
        },{
          "title"   : "着陆页",
          "name"    : "mainpage",
          "tip"     : "登录后默认跳转的页面",
          "width"   : "full",
          "comType" : "ti-input",
          "comConf" : {
            "placeholder" : "例如： page/home"
          }
        }],
      "updateBy" : "dispatch:main/updateCurrent",
      "setFieldStatusBy" : "commit:main/current/setMetaFieldStatus"
    }
  }
}