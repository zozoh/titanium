{
    "plugins": ["plugins/markdown"],
    "opts": {
        "template": "node_modules/tui-jsdoc-template"
    },
    "templates": {
        "logo": {
            "url" : "https://nutzam.com/core/logo.png",
            "link" : "http://github.com/zozoh/titanium"
        },
        "name": "Ti.API",
        "default": {
            "staticFiles": {
                "include": ["src/ti-doc-static/"]
            }
        },
        "css": [
            "styles/ti-doc.css"
        ],
        "footerText": "Titanium - a nice Vue ComLib &copy; Power by Nutz"
    }
}
